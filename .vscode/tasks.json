{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Build",
			"type": "shell",
			"command": "npm run build",
			"group": "build",
			"problemMatcher": ["$tsc"]
		},
		{
			"label": "Dev (watch)",
			"type": "shell",
			"command": "npm run dev",
			"isBackground": true
		},
		{
			"label": "Start (compiled)",
			"type": "shell",
			"command": "npm start"
		},
		{
			"label": "Test",
			"type": "shell",
			"command": "npm test -- --passWithNoTests",
			"problemMatcher": []
		}
	]
}